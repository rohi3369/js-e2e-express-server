pipeline {
    agent {label 'npm'}

    stages{
        stage('clone'){
            steps{
                git branch: 'main',
                url: 'https://github.com/rohi3369/js-e2e-express-server.git'
            }
        }
        stage('build'){
            steps{
                sh  'sudo apt install npm -y'
                sh 'npm install'
                sh  'npm start'
                }
        
        
        }

    }
}